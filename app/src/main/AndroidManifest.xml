<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.tanzeem.internity">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_icon"
        android:supportsRtl="true">
        <activity
            android:name=".LoginActivity.SplashScreen"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"
            tools:ignore="InnerclassSeparator">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity.LoginActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".InterviewerListActivity.InterviewerList"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".SchedulesActivity.ShivaniSingh"
            android:theme="@style/AppTheme.NoActionBar"
            android:parentActivityName=".InterviewerListActivity.InterviewerList"/>
        <activity
            android:name=".StudentActivity.StudentRegister"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".StudentActivity.StudentDashboard"
            android:label="@string/title_activity_student_dashboard"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".StudentActivity.ResultActivity"
            android:label="Result"
            android:parentActivityName=".StudentActivity.StudentDashboard"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".StudentActivity.FeedbackForm"
            android:parentActivityName=".StudentActivity.StudentDashboard"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".QuestionsActivity.Questions"
            android:theme="@style/AppTheme.NoActionBar"
            android:parentActivityName=".SchedulesActivity.ShivaniSinghCopy"/>

        <activity android:name=".SchedulesActivity.ShivaniSinghCopy"
            android:theme="@style/AppTheme.NoActionBar"/>

    </application>

</manifest>